<!--pages/book/book.wxml-->
<van-cell-group>
  <van-field
    label="房间："
    value="{{ value }}"
    placeholder="请输入房间号"
    border="{{ false }}"
    bind:click-input="onInputRoom"
  />
</van-cell-group>

<van-cell-group>
  <van-field
    label="日期："
    value="{{ value }}"
    placeholder="请输入会议室"
    border="{{ false }}"
    bind:change="onChange"
  />
</van-cell-group>

<van-cell-group>
  <van-field
    label="时间："
    value="{{ value }}"
    placeholder="请输入会议室"
    border="{{ false }}"
    bind:change="onChange"
  />
</van-cell-group>

<van-cell-group>
  <van-field
    label="创建者："
    value="{{ value }}"
    placeholder="请输入会议室"
    border="{{ false }}"
    bind:change="onChange"
  />
</van-cell-group>

<van-cell-group>
  <van-field
    label="会议内容："
    value="{{ value }}"
    placeholder="请输入会议室"
    border="{{ false }}"
    bind:change="onChange"
  />
</van-cell-group>



